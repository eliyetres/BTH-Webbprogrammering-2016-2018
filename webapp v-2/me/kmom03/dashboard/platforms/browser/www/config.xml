<?xml version='1.0' encoding='utf-8'?>
<widget id="se.dbwebb.dashboard" version="1.0.0" xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0">
    <name>Dashboard</name>
    <description>
        A sample Apache Cordova application that responds to the deviceready event.
    </description>
    <author email="dev@cordova.apache.org" href="http://cordova.io">
        Apache Cordova Team
    </author>
    <content src="index.html" />
    <access origin="*" />
    <allow-intent href="http://*/*" />
    <allow-intent href="https://*/*" />
    <allow-intent href="tel:*" />
    <allow-intent href="sms:*" />
    <allow-intent href="mailto:*" />
    <allow-intent href="geo:*" />
    <splash density="port-hdpi" src="www/res/mipmap-hdpi/ic_launcher.png" />
    <splash density="port-mdpi" src="www/res/mipmap-mdpi/ic_launcher.png" />
    <splash density="port-xhdpi" src="www/res/mipmap-xhdpi/ic_launcher.png" />
    <splash density="land-hdpi" src="www/res/mipmap-hdpi/ic_launcher.png" />
    <splash density="land-mdpi" src="www/res/mipmap-mdpi/ic_launcher.png" />
    <splash density="land-xhdpi" src="www/res/mipmap-xhdpi/ic_launcher.png" />
</widget>
